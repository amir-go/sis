<table class="table">
	<thead>
		<th>Name</th>
		<th>Email</th>
		<th>Year</th>
		<th>Department</th>
	</thead>

	<tbody>
		<% @users.each do |u| %>
		<tr>
			<td><%= u.full_name %></td>
			<td><%= u.email %></td>
			<td><%= u.year %></td>
			<td><%= u.department %></td>
			<td><%= link_to 'Edit', edit_admin_user_path(u.id) %></td>
		</tr>
		<% end %>
	</tbody>
</table>