<%= form_for @evaluation, url: admin_evaluations_path do |f| %>
	<div class="form-group">
		<%= f.label :user_id, class: 'control-label' %>
		<%= f.collection_select :user_id, User.all, :id, :full_name, { prompt: true }, class: 'form-control' %>
	</div>
	<div class="form-group">
		<%= f.label :subject_id, class: 'control-label' %>
		<%= f.collection_select :subject_id, Subject.all, :id, :name, { prompt: true }, class: 'form-control' %>
	</div>
	<div class="form-group">
		<%= f.label :evaluation, class: 'control-label' %>
		<%= f.text_field :evaluation, class: 'form-control' %>
	</div>
	<%= f.submit 'Save', class: 'btn btn-primary' %>
<% end %>